#include <stdio.h>
int main(){
    char palabra[30];
    int j=0;
    int letras=0;
    
    printf("Por favor, ingrese una palabra para saber cuantos caracteres posee.\n");
    scanf("%s",palabra);
    while(palabra[j++] != '\0')letras++;
    printf("La palabra que ingreso posee %d caracteres.", letras);    
    return 0;
}
